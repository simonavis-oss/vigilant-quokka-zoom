<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
  <!-- Printer base -->
  <rect x="4" y="18" width="24" height="10" rx="2"/>
  <!-- Print bed -->
  <rect x="6" y="20" width="20" height="6" rx="1" fill="currentColor" opacity="0.3"/>
  <!-- Printer frame -->
  <line x1="4" y1="18" x2="4" y2="8"/>
  <line x1="28" y1="18" x2="28" y2="8"/>
  <line x1="4" y1="8" x2="28" y2="8"/>
  <!-- Extruder assembly -->
  <rect x="12" y="10" width="8" height="4" rx="1"/>
  <rect x="13" y="14" width="6" height="2" rx="1"/>
  <!-- Filament spool -->
  <circle cx="24" cy="6" r="3"/>
  <circle cx="24" cy="6" r="2" fill="currentColor"/>
  <!-- Filament line -->
  <line x1="24" y1="6" x2="16" y2="10"/>
  <!-- Print head movement indicators -->
  <line x1="12" y1="16" x2="20" y2="16" stroke-dasharray="2 2"/>
  <!-- Small print object -->
  <rect x="14" y="22" width="4" height="2" fill="currentColor" opacity="0.5"/>
</svg>